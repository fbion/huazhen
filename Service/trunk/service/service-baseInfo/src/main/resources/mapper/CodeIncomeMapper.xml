<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hzfh.service.baseInfo.mapper.CodeIncomeMapper">
	<resultMap type="com.hzfh.api.baseInfo.model.CodeIncome" id="resultMapCodeIncome">
		<result property="id" column="id"/>
		<result property="inTime" column="in_time" jdbcType="TIMESTAMP" javaType="java.sql.Timestamp"/>
	</resultMap>
	<select id="getList" resultMap="resultMapCodeIncome">
		SELECT id,in_time
		FROM code_income order by in_time desc
	</select>
	<select id="getPagingList" resultMap="resultMapCodeIncome" parameterType="com.hzfh.api.baseInfo.model.query.CodeIncomeCondition" statementType="STATEMENT">
		SELECT id,in_time FROM code_income where 1=1
		ORDER BY
		<foreach item="item" collection="sortItemList" separator=",">
			${item.sortFeild} ${item.sortType}
		</foreach>
		LIMIT ${startIndex},${pageSize}
	</select>
	<select id="getTotalCount" resultType="int" parameterType="com.hzfh.api.baseInfo.model.query.CodeIncomeCondition" statementType="STATEMENT">
		SELECT count(1) FROM code_income where 1=1
	</select>
	<select id="getInfo" resultMap="resultMapCodeIncome" parameterType="int">
		SELECT id,in_time FROM code_income where id=#{id}
	</select>
	<insert id="add" parameterType="com.hzfh.api.baseInfo.model.CodeIncome" useGeneratedKeys="true" keyProperty="id">
		insert into code_income
		(in_time)
		values
		(current_timestamp())
	</insert>
	<update id="update" parameterType="com.hzfh.api.baseInfo.model.CodeIncome">
		update code_income set 
		
		where id=#{id}
	</update>
	<delete id="delete" parameterType="int">
		delete from code_income where id = #{id}
	</delete>
</mapper>